{
  "name": "@getlupa/client",
  "version": "1.1.7",
  "main": "dist/lupaSearch.js",
  "module": "dist/index.mjs",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "vite & yarn css:watch",
    "css:watch": "rollup --config build/rollupContainerDev.config.js --bundleConfigAsCjs --watch",
    "build": "yarn build-only && yarn css",
    "css": "rollup --config build/lupaStyle.config.js --bundleConfigAsCjs",
    "css:container": "rollup --config build/lupaContainerStyle.config.js --bundleConfigAsCjs",
    "cypress": "cypress",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test:e2e": "cypress run"
  },
  "devDependencies": {
    "@getlupa/client-sdk": "^1.3.1",
    "@getlupa/vue": "0.1.9",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^10.6.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.3",
    "prettier": "^2.8.8",
    "rollup-plugin-scss": "3",
    "sass": "^1.63.4",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vite-aliases": "^0.11.2",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  }
}
