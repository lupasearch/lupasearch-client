{
  "name": "@getlupa/client",
  "version": "1.18.5",
  "main": "dist/lupaSearch.js",
  "module": "dist/index.mjs",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "vite & yarn css:watch",
    "css:watch": "rollup --config build/rollupContainerDev.config.js --bundleConfigAsCjs --watch",
    "build": "yarn build-only && yarn css",
    "css": "rollup --config build/lupaStyle.config.js --bundleConfigAsCjs && rollup --config build/lupaChatStyle.config.js --bundleConfigAsCjs",
    "css:container": "rollup --config build/lupaContainerStyle.config.js --bundleConfigAsCjs",
    "cypress": "cypress",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test:e2e": "cypress run"
  },
  "devDependencies": {
    "@getlupa/client-sdk": "^1.8.1",
    "@getlupa/vue": "0.18.5",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.15",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.5.2",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.89.2",
    "start-server-and-test": "^2.0.12",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-aliases": "^0.11.8",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.3"
  },
  "packageManager": "yarn@1.22.19"
}
